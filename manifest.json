{
    "manifest_version": 2,
    "name": "Iframe Google Slides",
    "version": "0.0.2",
    "description": "Converts textbox URLs to Iframes. ",
    "content_scripts": [
      {
        "matches": ["https://docs.google.com/presentation/*"],
        "js": ["content.js"],
        "css": ["dist/styles.css"]
      }
    ],
    "icons": {
      "16": "logo/icon-16.png",
      "32": "logo/icon-32.png",
      "48": "logo/icon-48.png",
      "128": "logo/icon-128.png"
    }
}
